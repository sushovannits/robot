{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "plugin:@typescript-eslint/eslint-recommended",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": [2, {"args": "all", "argsIgnorePattern": "^_"}],
    "no-console": 1,
    "prettier/prettier": 2
  },
  "env" : {
    "jest": true
  },
  "globals": {
    "process": true
  }
}
